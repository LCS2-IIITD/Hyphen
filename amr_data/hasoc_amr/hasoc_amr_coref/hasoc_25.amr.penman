# ::lemmas ["Ram", "Rajya", "we", "come", "to", "lakshya", "dweep", ".", "secularist", "be", "scared", "."]
(c1-5 / multi-sentence
      :snt1 (c1-2 / come-01
                  :ARG1 (c1-3 / we)
                  :ARG4 (c1-0 / country-region
                              :name (c1-6 / name
                                          :op1 "Lakshya"
                                          :op2 "Dweep")))
      :snt2 (c1-1 / scare-01
                  :ARG1 (c1-4 / Secularist)))

